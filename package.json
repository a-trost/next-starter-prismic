{
  "name": "pigeon-loans-marketing-website",
  "version": "0.0.1",
  "description": "An awesome Slice Library made with Next.js",
  "author": "<author>",
  "license": "MIT",
  "main": "slices/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "prepublishOnly": "node ./node_modules/sm-commons/scripts/bundle",
    "sm": "prismic sm --develop",
    "sm:create": "prismic sm --create-slice",
    "storybook": "start-storybook -p 8888",
    "build-storybook": "build-storybook",
    "slicemachine": "start-slicemachine --port 9999",
    "format": "prettier --write \"{.slicemachine,.storybook,components,customtypes,docs,pages,slices,styles,utils}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "format:check": "prettier --check '{.slicemachine,.storybook,components,customtypes,docs,pages,slices,styles,utils}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}'",
    "lint": "eslint --fix '{components,pages,slices,utils}/**/*.js'",
    "lint:check": "eslint '{components,pages,slices,utils}/**/*.js'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.3",
    "@prismicio/client": "^5.1.0",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "chromatic": "^5.10.0-canary.0",
    "next": "^11.0.1",
    "next-slicezone": "^0.1.0",
    "next-transpile-modules": "^8.0.0",
    "prismic-cli": "^4.0.7",
    "prismic-javascript": "^3.0.2",
    "prismic-reactjs": "^1.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.6",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-require": "^3.1.3",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwind": "^0.2.0",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.2",
    "shx": "^0.3.3",
    "slice-machine-ui": "^0.1.0-beta.1",
    "sm-commons": "^0.0.23",
    "tailwindcss": "^2.2.4"
  }
}
